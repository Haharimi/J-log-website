extends layouts/main 
include mixins/videoBlock

block content 
    .search__videos 
        if videos.length === 0
            h4 "#{searchingBy}"로 검색된 결과가 없습니다.
        each item in videos
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl
            })
    