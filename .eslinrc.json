{
    "plugins": ["prettier"],
    "extends": ["eslint:recommended", "plugin:prettier/recommended","airbnb-base"],
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": false
        }
    },
    "env": {
        "browser": false,
        "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }